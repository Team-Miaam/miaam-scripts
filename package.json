{
	"name": "miaam-scripts",
	"version": "0.0.2",
	"description": "Miaam Scripts",
	"main": "index.js",
	"scripts": {
		"format": "prettier --write .",
		"lint": "eslint .",
		"lint:staged": "lint-staged --config .lintstagedrc.json",
		"build": "webpack --config ./webpack.dev.config.js",
		"test": "mocha",
		"test:watch": "mocha --watch || exit 0",
		"doc:contrib": "rm -rf ./docs/ && jsdoc -c ./contrib.jsdoc.json -R README.md",
		"doc:dev": "rm -rf ./docs/ && jsdoc -c ./dev.jsdoc.json -R README.md",
		"doc": "concurrently npm:doc:*",
		"prepare": "husky install"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"bin": {
		"miaam-scripts": "./bin/miaam-script"
	},
	"keywords": [
		"Miaam",
		"Web",
		"JavaScript",
		"Scripts"
	],
	"author": "Team Miaam Core",
	"license": "MIT",
	"devDependencies": {
		"chai": "^4.3.4",
		"husky": "^7.0.4",
		"lint-staged": "^12.1.3",
		"mocha": "^9.1.3",
		"webpack-cli": "^4.9.1"
	},
	"dependencies": {
		"@babel/core": "^7.16.0",
		"@babel/eslint-parser": "^7.16.3",
		"@babel/eslint-plugin": "^7.13.16",
		"@babel/preset-env": "^7.16.4",
		"@babel/register": "^7.16.0",
		"@pixi/jsdoc-template": "^2.6.0",
		"@swc/core": "^1.2.126",
		"babel-loader": "^8.2.3",
		"chalk": "^5.0.0",
		"commander": "^9.0.0",
		"concurrently": "^6.4.0",
		"cross-spawn": "^7.0.3",
		"esbuild": "^0.14.10",
		"eslint": "^7.26.0",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-prettier": "^3.4.1",
		"jsdoc": "^3.6.7",
		"prettier": "^2.4.1",
		"source-map-loader": "^3.0.0",
		"uglify-js": "^3.14.5",
		"webpack": "^5.69.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Team-Miaam/miaam-scripts"
	},
	"bugs": {
		"url": "https://github.com/Team-Miaam/miaam-scripts/issues"
	},
	"homepage": "https://github.com/Team-Miaam/miaam-scripts"
}
